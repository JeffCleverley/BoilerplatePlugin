{
    "name" 			: "Deftly/BoilerplatePlugin",
    "description" 	: "A Boilerplate Plugin to use as a foundation to develop from",
    "type"          : "project",
    "keywords" 		: ["wordpress", "wp", "plugin"],
    "homepage" 		: "https://jeffcleverley.com",
    "license" 		: "GPL-2.0+",
    "authors": [
        {
            "name" 	: "Jeff Cleverley",
            "email" : "jeff@jeffcleverley.com"
        }
    ],
    "require": {
        "php" : ">=5.6.0"
    },
    "require-dev": {

    },
    "autoload" : {
        "files": [
            "src/contents.php",
            "src/support/load_assets.php",
            "src/admin/admin.php"
        ]
    },
    "autoload-dev" : {
        "files": [
        
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}
